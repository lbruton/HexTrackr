{
  "spec": "007-kev-integration",
  "status": "PENDING", 
  "description": "CISA KEV catalog integration for vulnerability prioritization",
  "endpoints": [
    {
      "path": "/api/kev/sync",
      "method": "POST",
      "description": "Trigger KEV catalog synchronization",
      "response": {
        "schema": {
          "syncId": "string",
          "status": "string", 
          "lastSync": "string",
          "recordsUpdated": "number"
        }
      }
    },
    {
      "path": "/api/kev/status",
      "method": "GET",
      "description": "Get KEV integration status and statistics",
      "response": {
        "schema": {
          "lastSync": "string",
          "totalKevEntries": "number",
          "mappedVulnerabilities": "number", 
          "syncStatus": "string"
        }
      }
    },
    {
      "path": "/api/vulnerabilities/kev-enriched",
      "method": "GET", 
      "description": "Get vulnerabilities enriched with KEV data",
      "request": {
        "query": {
          "kevOnly": "boolean (default: false)",
          "limit": "number (default: 100)"
        }
      },
      "response": {
        "schema": {
          "vulnerabilities": [
            {
              "id": "string",
              "cve": "string",
              "isKev": "boolean",
              "kevDetails": "object|null"
            }
          ]
        }
      }
    }
  ]
}
{
  "spec": "020-snmp-inventory-system",
  "status": "PROPOSED",
  "description": "SNMP network device discovery and inventory endpoints",
  "endpoints": [
    {
      "path": "/api/snmp/discover",
      "method": "POST",
      "description": "Start network device discovery",
      "request": {
        "body": {
          "networkRange": "string",
          "credentials": "object"
        }
      },
      "response": {
        "schema": {
          "discoveryId": "string",
          "status": "string"
        }
      }
    },
    {
      "path": "/api/inventory/devices",
      "method": "GET", 
      "description": "Get discovered network devices",
      "response": {
        "schema": {
          "devices": [
            {
              "ip": "string",
              "hostname": "string",
              "type": "string",
              "lastSeen": "string"
            }
          ]
        }
      }
    }
  ]
}